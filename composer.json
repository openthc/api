			"OpenTHC\\API\\": "./lib",
			"OpenTHC\\API\\Test\\": "./test"
		}
	},
	"require": {
		"ext-yaml": "*",
		"justinrainbow/json-schema": "5.2.12",
		"openthc/common": "dev-master",
		"opis/json-schema": "2.3.0",
		"swaggest/json-schema": "0.12.39",
		"zircote/swagger-php": "4.4.4"
	},
	"require-dev": {
		"phpmd/phpmd": "2.12.0",
		"phpstan/phpstan": "1.7.8",
		"phpunit/phpunit": "9.5.20",
		"sebastian/phpcpd": "6.0.3"
	}
}
